{
  "iisSettings": {
    "windowsBank": false,
    "anonymousBank": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:54327",
      "sslPort": 44301
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "api/health/ping",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "BankService": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "api/health/ping",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
          "USERS_SERVICE_DNS": "localhost",
          "USERS_SERVICE_PORT": "5020",
          "ACCOUNTS_SERVICE_DNS": "localhost",
          "ACCOUNTS_SERVICE_PORT": "5010"
      },
      "applicationUrl": "https://localhost:5001;http://localhost:5000"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/api/health/ping"
    }
  }
}
